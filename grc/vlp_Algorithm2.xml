<?xml version="1.0"?>
<block>
  <name>Algorithm2</name>
  <key>vlp_Algorithm2</key>
  <category>[vlp]</category>
  <import>import vlp</import>
  <make>vlp.Algorithm2()</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
 <param>
    <name>Xtx1</name>
    <key>Xtx1</key>
    <type>float</type>
  </param>
  <param>
    <name>Ytx1</name>
    <key>Ytx1</key>
    <type>float</type>
  </param>
  <param>
    <name>Xtx2</name>
    <key>Xtx2</key>
    <type>float</type>
  </param>
  <param>
    <name>Ytx2</name>
    <key>Ytx2</key>
    <type>float</type>
  </param>
  <param>
    <name>Xtx3</name>
    <key>Xtx3</key>
    <type>float</type>
  </param>
  <param>
    <name>Ytx3</name>
    <key>Ytx3</key>
    <type>float</type>
  </param>
  <param>
    <name>Xtx4</name>
    <key>Xtx4</key>
    <type>float</type>
  </param>
  <param>
    <name>Ytx4</name>
    <key>Ytx4</key>
    <type>float</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in0</name>
    <type>float</type>
  </sink>
  <sink>
    <name>in1</name>
    <type>float</type>
  </sink>
  <sink>
    <name>in2</name>
    <type>float</type>
  </sink>
  <sink>
    <name>in3</name>
    <type>float</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out0</name>
    <type>float</type>
  </source>
  <source>
    <name>out1</name>
    <type>float</type>
  </source>
</block>
