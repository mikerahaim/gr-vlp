<?xml version="1.0"?>
<block>
  <name>Distance</name>
  <key>vlp_Distance</key>
  <category>VLP</category>
  <import>import vlp</import>
  <make>vlp.Distance($order_n, $A_phy, $Htx, $hrx, $m, $FOV, $Psi, $ampl, $CtCr0, $CtCr1, $CtCr2, $CtCr3, $G_f)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Order_N</name>
    <key>order_n</key>
    <type>float</type>
  </param>
  <param>
    <name>Area</name>
    <key>A_phy</key>
    <type>float</type>
  </param>
  <param>
    <name>Htx</name>
    <key>Htx</key>
    <type>float</type>
  </param>
  <param>
    <name>hrx</name>
    <key>hrx</key>
    <type>float</type>
  </param>
  <param>
    <name>Ind_Ref</name>
    <key>m</key>
    <type>float</type>
  </param>
  <param>
    <name>FOV</name>
    <key>FOV</key>
    <type>int</type>
  </param>
  <param>
    <name>Inc_Angle</name>
    <key>Psi</key>
    <type>int</type>
  </param>
  <param>
    <name>Ampl</name>
    <key>ampl</key>
    <type>float</type>
  </param>
  <param>
    <name>CtCr0</name>
    <key>CtCr0</key>
    <type>float</type>
  </param>
  <param>
    <name>CtCr1</name>
    <key>CtCr1</key>
    <type>float</type>
  </param>
  <param>
    <name>CtCr2</name>
    <key>CtCr2</key>
    <type>float</type>
  </param>
  <param>
    <name>CtCr3</name>
    <key>CtCr3</key>
    <type>float</type>
  </param>

  <param>
    <name>GainF</name>
    <key>G_f</key>
    <type>float</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in0</name>
    <type>float</type>
  </sink>
  
  <sink>
    <name>in1</name>
    <type>float</type>

  </sink>
  <sink>
    <name>in2</name>
    <type>float</type>
  </sink>

  <sink>
    <name>in3</name>
    <type>float</type>
  </sink>

  
  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out0</name>
    <type>float</type>
  </source>

  <source>
    <name>out1</name>
    <type>float</type>
  </source>

  <source>
    <name>out2</name>
    <type>float</type>
  </source>

  <source>
    <name>out3</name>
    <type>float</type>
  </source>
  
</block>
